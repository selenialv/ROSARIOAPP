@model IEnumerable<Matricula>

@{
    ViewData["Title"] = "estudiselect";
}

<table id="matriTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>

            <th>
                #
            </th>
            <th>
                Estudiante
            </th>

            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.IdestudianteNavigation.Idestudiante)
                </td>

                <td>
                    <a href="#myModal" class="btnEdit btn-primary btn-sm" data-toggle="modal"
                       data-id="@item.Idmatricula"><i class="fas fa-edit"></i></a>


                    <a href="#myModal" class="btnDelete btn-danger btn-sm" data-toggle="modal"
                       data-id="@item.Idmatricula"><i class="fas fa-trash-alt"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>